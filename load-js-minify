load('https://cdnjs.cloudflare.com/ajax/libs/html-minifier/3.3.3/htmlminifier.js')
minify = require('html-minifier').minify;
input = '<div> <p>    foo </p>    </div>';
output = minify(input, { collapseWhitespace: true });

